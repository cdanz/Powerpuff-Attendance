
@{
	ViewBag.Title = "Student Report";
}

<div class="col-md-10 col-md-offset-1 RectangleWhite">
	<div class="row align-center">
		<button class="section-scroll btn btn-g btn-round" value="Print" onclick="window.print();return false;" />Print</button>
	</div>

	<div class="row">
		<div align="center"><h2>Tamara's Progress</h2></div>
		<h4 align="center">Week: June 4th, 2018</h4>
		<div class="row align-center">
			<!-- Bar chart-->
			<div class="col-md-10 col-md-offset-1">
				<h4 align="center">This week's time in class...</h4>
				<br>
				<hr class="divider-w mt-10 mb-20">
				<h6 class="font-alt">
					<span class="icon-puzzle"></span> Monday
				</h6>
				<div class="progress">
					<div class="progress">
						<div class="progress-bar-success" role="progressbar" style="width: 100%;" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100">7 hrs</div>
					</div>
				</div>
				<h6 class="font-alt">
					<span class="icon-puzzle"></span> Tuesday
				</h6>
				<div class="progress">
					<div class="progress">
						<div class="progress-bar-success" role="progressbar" style="width: 100%;" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100">7 hrs</div>
					</div>
				</div>
				<h6 class="font-alt">
					<span class="icon-puzzle"></span> Wednesday
				</h6>
				<div class="progress">
					<div class="progress">
						<div class="progress-bar-success" role="progressbar" style="width: 0%;" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100">TBD</div>
					</div>
				</div>
				<h6 class="font-alt">
					<span class="icon-puzzle"></span> Thursday
				</h6>
				<div class="progress">
					<div class="progress">
						<div class="progress-bar-success" role="progressbar" style="width: 0%;" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100">TBD</div>
					</div>
				</div>
				<h6 class="font-alt">
					<span class="icon-puzzle"></span> Friday
				</h6>
				<div class="progress">
					<div class="progress">
						<div class="progress-bar-success" role="progressbar" style="width: 0%;" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100">TBD</div>
					</div>
				</div>
			</div>
		</div>

		<!-- Pie chart-->
		<div class="row align-center">
			<div class="col-md-10 col-md-offset-1">
				<div id="donutchart"></div>
			</div>
		</div>

		<div class="row align-center">
			<div id="barchart_values"></div>
		</div>
	</div>
</div>
